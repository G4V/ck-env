{
  "check_exit_status": "yes",
  "customize": {
    "install_env": {
      "CK_IMAGE_FILE": "",
      "PACKAGE_VERSION": "2012",
      "_CROP_FACTOR": "87.5",
      "_INPUT_SQUARE_SIDE": "224",
      "_DATA_TYPE": "uint8",
      "_NEW_EXTENSION": "rgb8",
      "_NORMALIZE_DATA": "0",
      "_SUBTRACT_MEAN": "0",
      "_GIVEN_CHANNEL_MEANS": "",
      "_SUBSET_FOF": "image_list.txt",
      "_SUBSET_OFFSET": "0",
      "_SUBSET_VOLUME": ""
    },
    "soft_uoa": "dataset.imagenet.preprocessed",
    "no_os_in_suggested_path": "yes",
    "no_ver_in_suggested_path": "yes",
    "skip_file_check": "yes"
  },
  "deps": {
    "dataset-source": {
      "enable_if_env": [
        {
          "CK_IMAGE_FILE": [
            ""
          ]
        }
      ],
      "local": "yes",
      "name": "ImageNet dataset (original)",
      "sort": 10,
      "tags": "dataset,imagenet,original"
    },
    "lib-python-numpy": {
      "local": "yes",
      "name": "Python NumPy library",
      "sort": 110,
      "tags": "lib,python-package,numpy"
    },
    "lib-python-cv2": {
      "local": "yes",
      "name": "Python OpenCV library",
      "sort": 120,
      "tags": "lib,python-package,cv2"
    },
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 100,
      "tags": "compiler,lang-python"
    }
  },
  "end_full_path_universal": "ILSVRC2012_val_00000001.JPEG",
  "only_for_host_os_tags": [],
  "only_for_target_os_tags": [],
  "process_script": "shell_wrapper",
  "suggested_path": "dataset-imagenet-preprocessed-using-opencv",
  "tags": [
    "image-classification",
    "dataset",
    "imagenet",
    "2012",
    "val",
    "preprocessed",
    "using-opencv"
  ],
  "variations": {
    "unnormalized": {
      "extra_env": {
        "_DATA_TYPE": "uint8",
        "_NEW_EXTENSION": "rgb8"
      },
      "extra_customize": {
        "soft_uoa": "dataset.imagenet.preprocessed"
      }
    },
    "normalized-for.mobilenet": {
      "extra_env": {
        "_DATA_TYPE": "float32",
        "_NEW_EXTENSION": "rgbf32",
        "_NORMALIZE_DATA": "YES",
        "_SUBTRACT_MEAN": "0"
      },
      "extra_customize": {
        "soft_uoa": "dataset.imagenet.preprocessed.rgbf32"
      }
    },
    "normalized-for.resnet": {
      "extra_env": {
        "_DATA_TYPE": "float32",
        "_NEW_EXTENSION": "rgbf32",
        "_NORMALIZE_DATA": "0",
        "_SUBTRACT_MEAN": "YES",
        "_GIVEN_CHANNEL_MEANS": "123.68 116.6 103.94"
      },
      "extra_customize": {
        "soft_uoa": "dataset.imagenet.preprocessed.rgbf32"
      }
    },
    "crop.1000": {
      "extra_env": {
        "_CROP_FACTOR": "100.0"
      }
    },
    "crop.875": {
      "extra_env": {
        "_CROP_FACTOR": "87.5"
      }
    },
    "external_file": {
      "extra_customize": {
        "package_name": "Pre-processed single image ($<<CK_IMAGE_FILE>>$)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": ""
      }
    },
    "first.20": {
      "extra_customize": {
        "package_extra_name": " (first 20)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "20"
      }
    },
    "full": {
      "extra_customize": {
        "package_extra_name": " (full)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": ""
      }
    },
    "last.20": {
      "extra_customize": {
        "package_extra_name": " (last 20)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "-20",
        "_SUBSET_VOLUME": ""
      }
    },
    "side.128": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "128"
      }
    },
    "side.160": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "160"
      }
    },
    "side.192": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "192"
      }
    },
    "side.224": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "224"
      }
    },
    "side.256": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "256"
      }
    }
  }
